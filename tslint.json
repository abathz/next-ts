{
  "extends": ["tslint-config-standard", "tslint-react"],
  "rules": {
    "await-promise": false,
    "deprecation": false,
    "no-use-before-declare": false,
    "no-floating-promises": false,
    "no-unnecessary-qualifier": false,
    "no-unnecessary-type-assertion": false,
    "strict-type-predicates": false
  }
}